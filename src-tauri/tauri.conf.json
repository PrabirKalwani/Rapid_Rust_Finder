{
  "build": {
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build",
    "devPath": "http://localhost:1420",
    "distDir": "../dist"
  },
  "package": {
    "productName": "rust-finder",
    "version": "0.1.0"
  },
  "tauri": {
    "allowlist": {
      "all": false,
      "shell": {
        "all": true
      }
    },
    "windows": [
      {
        "title": "rust-finder",

        "width": 800,
        "height": 600
      }
    ],
    "security": {
      "csp": null
    },
    "bundle": {
      "active": true,
      "targets": "all",
      "identifier": "com.rust-finder.app",
      "icon": ["/src-tauri/icons/icon.icns", "/src-tauri/icons/icon.ico"]
    }
  }
}
